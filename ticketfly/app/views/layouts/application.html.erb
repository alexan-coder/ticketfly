<!DOCTYPE html>
<html>
<head>
	<title>VenuMenu</title>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>
</head>
<body>

	<header>
		<div class="logo"> 
		<span id="svg"> <%= link_to image_tag( 'logo.svg' ), root_path %></span> </div>
		<form class="search" >
		  		<input class="phold" type="search" placeholder="search venue"> <br>
		  		<input class="hide" type="submit" value="go">
		</form>
		<nav class="buttons">
		
					<% if current_user %>
					<a href="#"> <%= current_user.name %> </a>
					<%= link_to "Sign out", signout_path %>
					<% else %>
					<%= link_to "Sign in with Twitter", "/auth/twitter" %>
					<% end %>
					<div>
				<a href='#'>Comments</a>
				
			</div>
			
		</nav>
	</header>


<!-- 	<div class="content">
		<h3 id="venues"></h3>
		<%= yield %>
	</div> -->
</body>
</html>